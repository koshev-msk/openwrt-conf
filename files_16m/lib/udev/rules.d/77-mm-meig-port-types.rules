# do not edit this file, it will be overwritten on update
ACTION!="add|change|move|bind", GOTO="mm_meig_port_types_end"
SUBSYSTEMS=="usb", ATTRS{idVendor}=="2dee", GOTO="mm_meig_port_types"
GOTO="mm_meig_port_types_end"

LABEL="mm_meig_port_types"

SUBSYSTEMS=="usb", ATTRS{bInterfaceNumber}=="?*", ENV{.MM_USBIFNUM}="$attr{bInterfaceNumber}"

# MeiG SLM828
#  ttyUSB0 (if #0): QCDM/DIAG port
#  ttyUSB1 (if #1): AT primary port
#  ttyUSB2 (if #2): AT secondary port
#  ttyUSB3 (if #3): GPS data port
ATTRS{idVendor}=="2dee", ATTRS{idProduct}=="4d22", ENV{.MM_USBIFNUM}=="00", ENV{ID_MM_PORT_TYPE_QCDM}="1"
ATTRS{idVendor}=="2dee", ATTRS{idProduct}=="4d22", ENV{.MM_USBIFNUM}=="01", ENV{ID_MM_PORT_TYPE_AT_PRIMARY}="1"
ATTRS{idVendor}=="2dee", ATTRS{idProduct}=="4d22", ENV{.MM_USBIFNUM}=="02", ENV{ID_MM_PORT_IGNORE}="1"
ATTRS{idVendor}=="2dee", ATTRS{idProduct}=="4d22", ENV{.MM_USBIFNUM}=="03", ENV{ID_MM_PORT_TYPE_GPS}="1" 

LABEL="mm_meig_port_types_end"

