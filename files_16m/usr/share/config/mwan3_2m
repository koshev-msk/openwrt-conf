
config globals 'globals'
	option mmx_mask '0x3F00'
	option rt_table_lookup '1 2 3'
	
config interface 'wan'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	list track_ip '132lan.ru'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option max_ttl '60'
	option timeout '4'
	option interval '60'
	option failure_interval '5'
	option recovery_interval '5'
	option down '5'
	option up '5'
	list flush_conntrack 'ifup'
	list flush_conntrack 'ifdown'
	list flush_conntrack 'connected'
	list flush_conntrack 'disconnected'

config interface 'modem1'
	option enabled '1'
	option family 'ipv4'
	list track_ip '132lan.ru'
	option track_method 'ping'
	option reliability '1'
	option size '56'
	option max_ttl '60'
	option timeout '4'
	option failure_interval '5'
	option recovery_interval '5'
	option count '3'
	option down '2'
	option up '2'
	option check_quality '0'
	option interval '30'
	option initial_state 'online'
	list flush_conntrack 'ifup'
	list flush_conntrack 'ifdown'
	list flush_conntrack 'connected'
	list flush_conntrack 'disconnected'

config interface 'modem2'
	option enabled '1'
	option family 'ipv4'
	option track_method 'ping'
	option reliability '1'
	option size '56'
	option max_ttl '60'
	option timeout '4'
	option failure_interval '5'
	option recovery_interval '5'
	list track_ip 'merona.ru'
	list track_ip '132lan.ru'
	option count '3'
	option keep_failure_interval '1'
	option down '2'
	option up '2'
	option initial_state 'online'
	option check_quality '0'
	option interval '30'
	list flush_conntrack 'ifup'
	list flush_conntrack 'ifdown'
	list flush_conntrack 'connected'
	list flush_conntrack 'disconnected'
	
