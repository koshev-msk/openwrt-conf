#!/bin/sh /etc/rc.common

START=90
USE_PROCD=1
NAME=torrserver
BINDIR=/mnt/disk/bin
PORT=8090

start_service(){
        procd_open_instance
        procd_set_param command ${BINDIR}/${NAME} -p ${PORT} -d ${BINDIR}/
        procd_set_param pidfile /var/run/${NAME}.pid
        procd_set_param stdout 1
        procd_close_instance
}

